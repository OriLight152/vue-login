{
  "name": "vue-login",
  "private": true,
  "type": "module",
  "scripts": {
    "server:dev": "json-server --watch src/server/db.json --delay 500",
    "vite:dev": "vite",
    "vite:build": "vue-tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "dev": "concurrently \"yarn vite:dev\" \"yarn server:dev\""
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "crypto-js": "^4.1.1",
    "js-cookie": "^3.0.1",
    "vue": "^3.2.37",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "json-server": "^0.17.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}